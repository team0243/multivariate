        "def time(t):\r\n",
        "  j3=  int (t.split('-')[0])*3600\r\n",
        "  k3= int (t.split('-')[1])*60\r\n",
        "  n3 = int (t.split('-')[2])\r\n",
        "  tt = j3+k3+n3\r\n",
        "  print(\"แปลงเป็นวินาทีได้เท่ากับ\",tt,\" วินาที\")"
      ],
